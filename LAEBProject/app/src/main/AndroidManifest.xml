        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LanguageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddNumberActivity" />
        <activity android:name=".VerificationCodeActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileActivity" />

        <activity android:name=".UpComingGamesActivity" />
        <activity android:name=".FullProfileActivity" />
